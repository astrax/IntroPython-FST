<!doctype html>
<html$if(lang)$ lang="$lang$"$endif$$if(dir)$ dir="$dir$"$endif$>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="chrome=1">
    <meta name="generator" content="pandoc">
$for(author-meta)$
    <meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
    <meta name="dcterms.date" content="$date-meta$">
$endif$
$if(description)$
    <meta name="description" content="$description$">
$endif$
$if(keywords)$
    <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$">
$endif$
$if(quotes)$
    <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
    <title>$title$</title>
    <link rel="stylesheet" href="stylesheets/styles.css">
    <link rel="stylesheet" href="stylesheets/highlight.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
    <script src="javascripts/main.js"></script>
    <script src="javascripts/highlight.pack.js"></script>
    <script>hljs.initHighlightingOnLoad();</script>
$for(css)$
    <link rel="stylesheet" href="stylesheets/$css$">
$endfor$
$if(math)$
    $math$
$endif$
    <!--[if lt IE 9]>
      <script src="//html5shiv.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
$for(header-includes)$
    $header-includes$
$endfor$
  </head>
  <body>

  <header>
    <h1>$website_title$</h1>
    <p>$website_tagline$</p>
  </header>

  <div id="banner">
    <div class="banner-inner">
      <span id="logo"></span>

      <a href="https://github.com/$your_github_username$/$your_github_reponame$" class="button fork"><strong>Fork On GitHub</strong></a>
      <div class="downloads">
        <span>Downloads:</span>
        <ul>
          <li><a href="https://github.com/$your_github_username$/$your_github_reponame$/archive/master.zip" class="button">ZIP</a></li>
          <li><a href="https://github.com/$your_github_username$/$your_github_reponame$/archive/master.tar.gz" class="button">TAR</a></li>
        </ul>
      </div>
    </div>
  </div><!-- end banner -->

  <div class="wrapper">
  <nav>
      <ul></ul>
    </nav>

    <section>

$for(include-before)$
$include-before$
$endfor$
$body$
$for(include-after)$
$include-after$
$endfor$

      </section>

      <footer>
        <p><span class="small">Generated with<br><a href="https://tajmone.github.io/gh-themes-magick/"><strong>gh-themes-magick</strong></a><br>&mdash; Theme by <a href="http://twitter.com/#!/michigangraham">mattgraham</a></span></p>
      </footer>
    </div>
    <!--[if !IE]><script>fixScale(document);</script><!--<![endif]-->
  </body>
</html>
